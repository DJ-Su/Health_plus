<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:id="@+id/bio_scroll"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
<androidx.constraintlayout.widget.ConstraintLayout
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".admin.AdminFoodAdd"
android:background="#FFF5B3">
<ImageView
    android:id="@+id/imageView"
    android:layout_width="0dp"
    android:layout_height="?attr/actionBarSize"
    android:background="@color/white"
    app:srcCompat="@drawable/logo3"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintEnd_toEndOf="parent"/>

<TextView
    android:id="@+id/FoodNameAdd"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="72dp"
    android:fontFamily="@font/a1009_2"
    android:text="음식명"
    android:textColor="@color/black"
    android:textSize="24dp"
    android:layout_marginTop="38dp"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <EditText
        android:id="@+id/FoodNameEdit"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:gravity="center_horizontal|center_vertical"
        android:hint="음식명"
        app:layout_constraintBottom_toBottomOf="@+id/FoodNameAdd"
        app:layout_constraintStart_toEndOf="@+id/FoodNameAdd"
        app:layout_constraintTop_toTopOf="@+id/FoodNameAdd" />

<TextView
    android:id="@+id/FoodKcalAdd"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="72dp"
    android:fontFamily="@font/a1009_2"
    android:text="칼로리"
    android:textColor="@color/black"
    android:textSize="24dp"
    android:layout_marginTop="40dp"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/FoodNameAdd" />

    <EditText
        android:id="@+id/FoodKcalEdit"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:gravity="center_horizontal|center_vertical"
        android:hint="칼로리"
        app:layout_constraintBottom_toBottomOf="@+id/FoodKcalAdd"
        app:layout_constraintStart_toEndOf="@+id/FoodKcalAdd"
        app:layout_constraintTop_toTopOf="@+id/FoodKcalAdd" />

<TextView
    android:id="@+id/FoodCarbohydrateAdd"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="48dp"
    android:fontFamily="@font/a1009_2"
    android:text="탄수화물"
    android:textColor="@color/black"
    android:textSize="24dp"
    android:layout_marginTop="40dp"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/FoodKcalAdd" />

    <EditText
        android:id="@+id/FoodCarbohydrateEdit"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:gravity="center_horizontal|center_vertical"
        android:hint="탄수화물"
        app:layout_constraintBottom_toBottomOf="@+id/FoodCarbohydrateAdd"
        app:layout_constraintStart_toEndOf="@+id/FoodCarbohydrateAdd"
        app:layout_constraintTop_toTopOf="@+id/FoodCarbohydrateAdd" />

<TextView
    android:id="@+id/FoodProteinAdd"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="71dp"
    android:fontFamily="@font/a1009_2"
    android:text="단백질"
    android:textColor="@color/black"
    android:textSize="24dp"
    android:layout_marginTop="40dp"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/FoodCarbohydrateAdd" />

    <EditText
        android:id="@+id/FoodProteinEdit"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:gravity="center_horizontal|center_vertical"
        android:hint="단백질"
        app:layout_constraintBottom_toBottomOf="@+id/FoodProteinAdd"
        app:layout_constraintStart_toEndOf="@+id/FoodProteinAdd"
        app:layout_constraintTop_toTopOf="@+id/FoodProteinAdd" />

<TextView
    android:id="@+id/FoodFatAdd"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="94dp"
    android:fontFamily="@font/a1009_2"
    android:text="지방"
    android:textColor="@color/black"
    android:textSize="24dp"
    android:layout_marginTop="40dp"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/FoodProteinAdd" />

    <EditText
        android:id="@+id/FoodFatEdit"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:gravity="center_horizontal|center_vertical"
        android:hint="지방"
        app:layout_constraintBottom_toBottomOf="@+id/FoodFatAdd"
        app:layout_constraintStart_toEndOf="@+id/FoodFatAdd"
        app:layout_constraintTop_toTopOf="@+id/FoodFatAdd" />

<TextView
    android:id="@+id/FoodSodiumAdd"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="72dp"
    android:fontFamily="@font/a1009_2"
    android:text="나트륨"
    android:textColor="@color/black"
    android:textSize="24dp"
    android:layout_marginTop="40dp"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/FoodFatAdd" />

    <EditText
        android:id="@+id/FoodSodiumEdit"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:gravity="center_horizontal|center_vertical"
        android:hint="나트륨"
        app:layout_constraintBottom_toBottomOf="@+id/FoodSodiumAdd"
        app:layout_constraintStart_toEndOf="@+id/FoodSodiumAdd"
        app:layout_constraintTop_toTopOf="@+id/FoodSodiumAdd"
        app:layout_constraintVertical_bias="0.444" />

    <TextView
    android:id="@+id/FoodSugarAdd"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="117dp"
    android:fontFamily="@font/a1009_2"
    android:text="당"
    android:textColor="@color/black"
    android:textSize="24dp"
    android:layout_marginTop="40dp"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/FoodSodiumAdd" />

    <EditText
        android:id="@+id/FoodSugarEdit"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:gravity="center_horizontal|center_vertical"
        android:hint="당"
        app:layout_constraintBottom_toBottomOf="@+id/FoodSugarAdd"
        app:layout_constraintStart_toEndOf="@+id/FoodSugarAdd"
        app:layout_constraintTop_toTopOf="@+id/FoodSugarAdd" />

    <TextView
        android:id="@+id/FoodgAdd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:fontFamily="@font/a1009_2"
        android:text="기준(무게)"
        android:textColor="@color/black"
        android:textSize="24dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/FoodSugarAdd" />

    <EditText
        android:id="@+id/FoodgEdit"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:gravity="center_horizontal|center_vertical"
        android:hint="기준(무게)"
        app:layout_constraintBottom_toBottomOf="@+id/FoodgAdd"
        app:layout_constraintStart_toEndOf="@+id/FoodgAdd"
        app:layout_constraintTop_toTopOf="@+id/FoodgAdd"
        app:layout_constraintVertical_bias="0.444" />

    <ImageButton
        android:id="@+id/ImgaddBtn"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:background="#75B8B8B8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/FoodgEdit"
        android:layout_marginTop="30dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="사진 불러오기"
        android:textColor="#2E2E2E"
        android:textSize="22dp"
        android:fontFamily="@font/a1009_2"
        app:layout_constraintBottom_toBottomOf="@+id/ImgaddBtn"
        app:layout_constraintEnd_toEndOf="@+id/ImgaddBtn"
        app:layout_constraintStart_toStartOf="@+id/ImgaddBtn"
        app:layout_constraintTop_toTopOf="@+id/ImgaddBtn" />

    <ImageButton
        android:id="@+id/addBtn"
        android:layout_width="130dp"
        android:layout_height="40dp"
        android:layout_marginTop="52dp"
        android:background="#00000000"
        android:src="@drawable/roundsquare3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ImgaddBtn" />

    <Button
        android:id="@+id/SaveBtn"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:fontFamily="@font/a10093"
    android:text="추가하기"
    android:textColor="#2E2E2E"
        android:backgroundTint="#ff9898"
    android:textSize="24dp"
    app:layout_constraintBottom_toBottomOf="@+id/addBtn"
    app:layout_constraintEnd_toEndOf="@+id/addBtn"
    app:layout_constraintStart_toStartOf="@+id/addBtn"
    app:layout_constraintTop_toTopOf="@+id/addBtn" />




</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>